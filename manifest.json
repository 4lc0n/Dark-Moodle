{
    "manifest_version": 2,
    "name": "Dark Moodle",
    "version": "0.1",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "description": "Dark theme for UBS Moodle",
    "content_scripts": [
        {
            "css": ["./content.css"],
            "matches": ["https://moodle.univ-ubs.fr/*"]
        }
    ],
    "permissions": ["tabs"]
}